{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true,
      "browser": true
    },
    "extends": [ "airbnb-base", "prettier" ], //this is the part the determines the eslint to follow
    "plugins": ["prettier"],
    "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": "error",
      "no-console": "off"
    }
  }

